# Cloak SDK Configuration
# Copy this file to .env and fill in your values

# ====================
# Solana Configuration
# ====================

# Path to your Solana keypair JSON file
# Default: ~/.config/solana/id.json
# To generate a new keypair: solana-keygen new
KEYPAIR_PATH=

# Solana RPC endpoint URL
# Default: https://devnet.helius-rpc.com/?api-key=YOUR_API_KEY
# Popular options:
#   - Devnet: https://api.devnet.solana.com
#   - Mainnet: https://api.mainnet-beta.solana.com
#   - Helius: https://devnet.helius-rpc.com/?api-key=YOUR_API_KEY
#   - QuickNode: Your custom QuickNode endpoint
RPC_URL=

# Solana cluster environment
# Options: "devnet" or "mainnet"
# Default: mainnet (if not set)
# This affects the ALT_ADDRESS selection
SOLANA_CLUSTER=

# ====================
# Cloak Protocol Configuration
# ====================

# Relayer URL (optional)
# The SDK will use the default dev relayer if not specified
# Only set this if using a custom relayer instance
relayer_API_URL=

